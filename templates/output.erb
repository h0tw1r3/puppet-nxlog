<Output <%= @output_name%>>
    Module  om_<%= @output_module %>
<% if @output_file -%>
    File    <%= @output_file %>
<% end -%>
<% if @output_host -%>
    Host    <%= @output_host %>
<% end -%>
<% if @output_port -%>
    Port    <%= @output_port %>
<% end -%>
<% if @output_type -%>
    OutputType <%= @output_type %>
<% end -%>
<% [*@output_execs].each do |output_exec| -%><%="    Exec    #{output_exec};" -%>
<% [*@output_options].each do |output_option| -%><%="    #{output_option}" %>
</Output>
