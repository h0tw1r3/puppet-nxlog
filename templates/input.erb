<Input <%= @input_name %>>
    Module    im_<%= @input_module %>
<% if @query -%>
    Query     <%= @query %>
<% end -%>
<% if @input_file -%>
    File      <%= @input_file %>
<% end -%>
<% if @input_type -%>
    InputType <%= @input_type %>
<% end -%>
<% [*@input_execs].each do |input_exec| -%><%="    Exec      #{input_exec};" %>
<% end -%>
<% [*@input_options].each do |input_option| -%><%="    #{input_option}" %>
<% end -%>
</Input>
